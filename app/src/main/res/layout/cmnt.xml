<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_marginTop="1dp"
    android:layout_marginBottom="1dp"
    android:background="@drawable/dsn44"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:id="@+id/tabllayout"
        android:background="#fff"
        >

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginLeft="5dp"
            android:id="@+id/main"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/over"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="103.2"
                android:textColor="#111"
                android:textSize="17dp"
                android:typeface="sans"
                app:fontFamily="sans-serif-medium"
                android:layout_marginLeft="0dp"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_below="@+id/main"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
           android:id="@+id/main1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="4dp"
                android:layout_marginTop="4dp"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="9dp"
                android:layout_centerHorizontal="true"
                android:gravity="center"
                android:text="W"
                android:textColor="#fff"
                android:textSize="14dp"
                android:typeface="sans"
                android:fontFamily="sans-serif-medium" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_marginRight="15dp"
            android:layout_toEndOf="@+id/main"
            android:layout_marginLeft="6dp"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/cmnts"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:text="R23"
                android:textColor="#111"
                android:textSize="16dp"
                android:typeface="sans"
                app:fontFamily="sans-serif" />
            <TextView
                android:id="@+id/cmntsd"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@+id/cmnts"
                android:layout_marginLeft="6dp"
                android:gravity="center"
                android:text="Clean Bowled"
                android:textColor="#111"
                android:textSize="14dp"
                android:typeface="sans"
                app:fontFamily="sans-serif-medium" />
            <TextView
                android:id="@+id/temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_toEndOf="@+id/cmnts"
                android:layout_marginLeft="6dp"
                android:gravity="center"
                android:textStyle="bold"
                android:visibility="gone"
                android:text="R23"
                android:textColor="#111"
                android:textSize="14dp"
                android:typeface="sans"
                app:fontFamily="sans-serif-medium" />

        </RelativeLayout>




    </RelativeLayout>
    <View
        android:id="@+id/divider3"
        android:layout_below="@+id/tabllayout"
        android:layout_width="match_parent"
        android:layout_height="0.2dp"
        android:background="#D3CDCD"

        />


</RelativeLayout>